package com.mime.minefront;

import java.io.File;
import java.io.OutputStream;

public class Configuration {

	public void saveConfigurtion(String key, int value) {

		try {

			File file = new File("res/settings/config.xml");
			boolean exists = file.exists();
			if (!exists) {
				file.createNewFile();
			}
			OutputStream 
		} catch (Exception e) {

			e.printStackTrace();
		}
	}
}
