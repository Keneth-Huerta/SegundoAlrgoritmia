package modelo;

import java.sql.Connection;
import java.sql.PreparedStatement;

import controlador.Conexion;

public class ModificaConsulta {
	private Conexion miConexion;
	private PreparedStatement enviaCambio;
	private final String instruccionSQL = "UPDATE FROM articulos WHERE CodigoArticulos=?";

	public ModificaConsulta() {

		miConexion = new Conexion();
	}

	public String cambiaContenido() {
		String prueba;
		Connection conecta = miConexion.dameConexion();
		prueba = null;
		try {
			enviaCambio=conecta.prepareStatement(instruccionSQL);
			

		} catch (Exception e) {
			e.getMessage();
			e.printStackTrace();
		}
		return prueba;
	}
}
