package clases_propias;

public class UsoPareja {

	public static void main(String[] args) {

		Pareja<String> una = new Pareja<String>();
		una.setPrimero("Juan");
		System.out.println(una.getPrimero());
		
		Pareja
	}

}

class Persona {
	private String nombre;

	public Persona(String nombre) {

		this.nombre = nombre;
	}
}
